{
  "name": "ossome",
  "version": "0.0.1",
  "description": "AI-Powered Team Communication for Jiu-Jitsu Gyms",
  "productName": "Ossome",
  "author": "Elvis Ibarra <ielvisd@gmail.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:critical": "playwright test critical-path.spec.ts",
    "test:e2e:ios": "playwright test ios/",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "pnpm test:run && pnpm test:e2e",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare",
    "prepare": "husky",
    "db:apply": "./scripts/apply-sql.sh",
    "db:apply-migrations": "./scripts/apply-sql.sh MIGRATIONS_TO_APPLY_MANUALLY.sql",
    "demo:create": "node scripts/create-demo-accounts.js",
    "demo:create-owner": "node scripts/create-owner-and-instructors.js",
    "test:supabase": "node scripts/test-supabase-connection.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.53",
    "@ai-sdk/vue": "^2.0.76",
    "@capacitor/app": "^7.1.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.3",
    "@quasar/extras": "^1.16.4",
    "@supabase/supabase-js": "^2.76.0",
    "@types/qrcode": "^1.5.5",
    "ai": "^5.0.76",
    "axios": "^1.2.1",
    "bcryptjs": "^3.0.2",
    "jsqr": "^1.4.0",
    "openai": "^6.6.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "quasar": "^2.16.0",
    "vue": "^3.5.22",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@playwright/test": "^1.48.0",
    "@quasar/app-vite": "^2.1.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "supabase": "^2.51.0",
    "typescript": "^5.9.2",
    "vite-plugin-checker": "^0.11.0",
    "vite-plugin-vue-mcp": "^0.3.2",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
