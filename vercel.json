{
  "version": 2,
  "buildCommand": "cd messageAI-Quasar && rm -f pnpm-lock.yaml && npm ci && npx quasar build -m pwa",
  "outputDirectory": "messageAI-Quasar/dist/pwa",
  "installCommand": "cd messageAI-Quasar && npm install",
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|json|webmanifest))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

