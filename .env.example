# MessageAI Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration
VITE_API_BASE_URL=https://api.messageai.com
VITE_API_VERSION=v1

# App Configuration
VITE_APP_NAME=MessageAI
VITE_APP_VERSION=0.0.1
VITE_APP_DESCRIPTION=AI-Powered Cross-Platform Messaging App for Remote Teams

# Development Configuration
VITE_DEBUG=false
VITE_LOG_LEVEL=info

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_CAMERA=true

# Third-party Services (if needed)
# VITE_OPENAI_API_KEY=your_openai_api_key_here
# VITE_ANTHROPIC_API_KEY=your_anthropic_api_key_here
